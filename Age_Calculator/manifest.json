{
    "manifest_version": 2,
    "name": "Age Calculator",
    "version": "2.0.1",
    "description": "This is a simple extension that is Age calculator",
    "author": "Dev Rishik Maruturi",
    "icons": {
      "128": "calculator.png"
    },
  
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "calculator.png",
      "default_title": "Age Calculator"
    },
  
    "permissions": ["activeTab"],
  
    "background": {
      "scripts": ["script.js"]
    }
  }